	iif(F.Size = 0, '',
		convert(varchar(10),
			Round( convert(decimal(5, 2) ,
				(F.Remaining/F.Size) * 100 ), 2)) + '%') as RemainingRate,
	convert(varchar(40),
		convert(decimal(10, 2),
