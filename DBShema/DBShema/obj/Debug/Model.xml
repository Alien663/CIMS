		<Element Type="SqlView" Name="[dbo].[vd_MachineUsers]">
			<Property Name="QueryScript">
				<Value><![CDATA[
select R.RID, R.MachineName, F.[Group],
	REPLACE(F.Account, '\\', '\') as [Account],
	iif(CHARINDEX('QUANTA', F.Account) > 0, 1, 0) as isDomain,
	case F.isActive
